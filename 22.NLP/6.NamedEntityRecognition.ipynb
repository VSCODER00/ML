   "source": [
    "from nltk import ne_chunk,pos_tag,word_tokenize\n",
    "sentence=\"Taj Mahal is a beautiful monument\"\n",
    "words=word_tokenize(sentence)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "dc1f823e",
   "metadata": {},
   "outputs": [],
   "source": [
    "tags=pos_tag(words)\n",
    "op=ne_chunk(tags)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "c48014dd",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "(S\n",
      "  (PERSON Taj/NNP)\n",
      "  (ORGANIZATION Mahal/NNP)\n",
      "  is/VBZ\n",
      "  a/DT\n",
      "  beautiful/JJ\n",
      "  monument/NN)\n"
     ]
    }
   ],
   "source": [
    "print(op)"
   ]
  },
